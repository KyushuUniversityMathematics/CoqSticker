From mathcomp Require Import all_ssreflect.
Require Import Ascii String.

Open Scope nat_scope.

(* finType として使う型を定義する例 *)
Inductive my_type := A | B | C.

(* my_type を finType として定義する *)
Instance my_type_finType : finType := FinType (EqType my_type).

(* 長さ2のリストに finType を持たせる *)
Definition finlist2_type := list my_type.

(* finlist2_type を finType として定義する *)
Instance finlist2_type_finType : finType := FinType (EqType finlist2_type)
